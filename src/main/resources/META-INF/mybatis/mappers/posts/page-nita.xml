<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="khhits.repositories.PageRepository">

    <insert id="save">
        INSERT INTO page(page_id, page_title, id)
        VALUES (
        #{page_id},
        #{page_title},
        #{id}
        )
    </insert>

</mapper>
